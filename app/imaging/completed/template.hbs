{{#item-listing paginationProps=paginationProps}}
  {{#if model}}
    <table class="table imaging-completed-table">
      <tr class="table-header">
        <th>{{t 'models.imaging.labels.requestedDate'}}</th>
        <th>{{t 'models.imaging.labels.imagingDate'}}</th>
        <th>{{t 'models.imaging.labels.requestedBy'}}</th>
        <th>{{t 'models.imaging.labels.patient'}}</th>
        <th>{{t 'models.imaging.labels.imagingType'}}</th>
        <th>{{t 'models.imaging.labels.result'}}</th>
        <th>{{t 'models.imaging.labels.notes'}}</th>
      </tr>
      {{#each model as |imaging|}}
        {{imaging/completed/completed-list-item imaging=imaging}}
      {{/each}}
    </table>
  {{else}}
    <div class="alert alert-info">
      <p>{{t 'imaging.messages.noCompleted'}}</p>
    </div>
  {{/if}}
{{/item-listing}}
